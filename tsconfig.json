{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "skipLibCheck": true,
    "types": ["node", "cypress", "webpack-env"]
  },
  "include": [
    "src/components/index.tsx",
    "cypress/**/*.ts."
  ]
}
