{
  "name": "cypress-playground",
  "version": "1.0.0",
  "description": "playground for learning cypress and acessibility technologies",
  "main": "src/index.tsx",
  "type": "module",
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --mode development",
    "build": "webpack --config ./webpack.config.js --mode production",
    "cy:open": "cypress open",
    "cy:debug": "DEBUG=cypress:* cypress open",
    "cy:axe": "start-server-and-test start http://localhost:8080 cy:open",
    "cy:axe:webpage": "axe http://localhost:8080 --timeout=120 --save axe/webpage-results/results.json  --tags wcag2a",
    "lint": "eslint . --ext .tsx",
    "pa11y": "pa11y http://localhost:8080",
    "pa11y:508": "pa11y --standard Section508 http://localhost:8080",
    "pa11y:axe": "pa11y --runner axe http://localhost:8080",
    "pa11y:reporter:json": "pa11y --reporter json http://localhost:8080 > axe/pa11y/report.json",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.tsx' --write",
    "prettier-watch": "onchange 'src/**/*.tsx' -- prettier --write {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michael-mclaughlin/cypress-playground.git"
  },
  "keywords": [],
  "author": {
    "name": "mike-mclaughlin",
    "email": "mclaughlin@indeed.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michael-mclaughlin/cypress-playground/issues"
  },
  "homepage": "https://github.com/michael-mclaughlin/cypress-playground#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/html2canvas": "0.0.36",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "axe-core": "^4.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-styled-components": "^1.11.1",
    "css-loader": "^5.0.1",
    "cypress": "^4.12.1",
    "cypress-axe": "^0.10.0",
    "cypress-plugin-retries": "^1.5.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fast-deep-equal": "^3.1.3",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.1",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "nodemon": "^2.0.6",
    "onchange": "^7.1.0",
    "pa11y-lint-config": "^1.3.0",
    "prettier": "2.1.2",
    "prop-types": "^15.7.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^17.0.1",
    "source-map-loader": "^1.1.2",
    "start-server-and-test": "^1.11.5",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.0.5",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/node": "^14.14.8",
    "classnames": "^2.2.6",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "html2canvas": "^1.0.0-rc.7",
    "pa11y-reporter-cli": "^2.0.0",
    "pa11y-reporter-csv": "^2.0.0",
    "pa11y-reporter-json": "^2.0.0",
    "pa11y-runner-axe": "^2.0.0",
    "pa11y-runner-htmlcs": "^2.0.0",
    "patternomaly": "^1.3.2",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-outside-click-handler": "^1.3.0",
    "react-router-dom": "^5.2.0",
    "react-with-direction": "^1.3.1",
    "reload": "^3.1.1",
    "styled-components": "^5.2.1",
    "ts-loader": "^8.0.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --fix-dry-run"
    ]
  }
}
